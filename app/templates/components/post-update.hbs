<h1>Update Post</h1>
<form>
  <div class="form-group">
    <label for="title">Title</label>
    <input id="title" type="text" placeholder=title>
  </div>
  <div class="form-group">
    <label for="natureType">Nature Type</label>
    <input id="natureType" type="text" placeholder=post.natureType>
  </div>
  <div class="form-group">
    <label for="trevorSong">Trevor's Song</label>
    <input id="trevorSong" type="text" placeholder=post.trevorSong>
  </div>
  <div class="form-group">
    <label for="dustinSong">Dustin's Song</label>
    <input id="dustinSong" type="text" placeholder=post.dustinSong>
  </div>
  <div class="form-group">
    <label for="tileImage">Tile Image</label>
    <input id="tileImage" type="text" placeholder=post.tileImage>
  </div>
  <div class="form-group">
    <label for="images">Post Images</label>
    <input id="images" type="text" placeholder=post.images>
  </div>
</form>
